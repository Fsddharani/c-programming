// swapping
#include<stdio.h>
#include<conio.h>

void main()
{
    int a,b;
    printf("enter the value of a:");
    scanf("%d",&a);
    printf("\nenter the value of b:");
    scanf("%d",&b);
    a=a+b;
    b=a-b;
    a=a-b;
    printf("the value of a after swapping is %d",a);
    printf("\nthe value of b after swapping is %d",b);
    getch();
}
